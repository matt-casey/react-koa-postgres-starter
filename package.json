{
  "name": "react-redux-sass-seed",
  "version": "1.0.0",
  "description": "Starter project for React/Redux with custom SCSS",
  "main": "app/index.js",
  "scripts": {
    "copy-assets": "mkdir -p build && cp app/index.html build/index.html && cp -R app/favicon.ico build/favicon.ico && cp -R app/assets build/assets",
    "build:api": "rimraf compiled && babel server --out-dir compiled",
    "build:web": "rimraf build && npm run copy-assets && webpack --config webpack.prod.config.js --progress --profile --colors",
    "build": "npm run build:api && npm run build:web",
    "test": "mocha ./test/ --recursive --compilers js:babel-register --reporter dot",
    "coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-register --reporter dot ./test/",
    "start:dev": "NODE_ENV=development nodemon --watch server server/index.js --exec babel-node --preserve-lines",
    "start": "NODE_ENV=production node compiled/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matt-casey/react-redux-sass-seed.git"
  },
  "keywords": [
    "react",
    "redux",
    "sass",
    "seed"
  ],
  "author": "Matt Casey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matt-casey/react-redux-sass-seed/issues"
  },
  "homepage": "https://github.com/matt-casey/react-redux-sass-seed#readme",
